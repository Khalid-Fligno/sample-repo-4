    "orientation": "portrait",
    "icon": "./assets/icons/fitazfk-app-icon-gradient-dark.png",
    "splash": {
      "image": "./assets/icons/fitazfk-splash-dark-logo.png",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.fitazfk.fitazfkapp",
      "buildNumber": "1",
      "publishBundlePath": "ios/fitazfk/Supporting/shell-app.bundle",
      "publishManifestPath": "ios/fitazfk/Supporting/shell-app-manifest.json",
      "usesAppleSignIn": true,
      "icon": "./assets/icons/fitazfk-app-icon-gradient-dark.png"
    },
    "android": {
      "package": "com.fitazfk.fitazfkapp",
      "publishBundlePath": "android/app/src/main/assets/shell-app.bundle",
      "publishManifestPath": "android/app/src/main/assets/shell-app-manifest.json"
    },
    "scheme": "fitazfk",
    "platforms": [
      "android",
      "ios"
    ],
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
